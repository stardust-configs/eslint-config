{
  "name": "@stardust-configs/eslint-config",
  "version": "0.9.0",
  "description": "Shareable ESLint config",
  "license": "MIT",
  "author": "P-Chan",
  "files": [
    "**/*.d.ts",
    "**/*.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/stardust-configs/eslint-config.git"
  },
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rimraf --glob \"./{,!(node_modules)/**/}*.{js,d.ts}\"",
    "dev": "tsc --watch",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepublishOnly": "npm run build",
    "version:major": "standard-version -r major",
    "version:minor": "standard-version -r minor",
    "version:patch": "standard-version -r patch"
  },
  "dependencies": {
    "@next/eslint-plugin-next": "13.1.6",
    "@typescript-eslint/eslint-plugin": "5.50.0",
    "@typescript-eslint/parser": "5.50.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsonc": "2.7.0",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-tailwindcss": "3.10.1"
  },
  "devDependencies": {
    "@stardust-configs/prettier-config": "0.1.1",
    "@stardust-configs/tsconfig": "0.3.0",
    "eslint": "8.37.0",
    "prettier": "2.8.7",
    "rimraf": "4.4.1",
    "standard-version": "9.5.0",
    "typescript": "4.9.5"
  },
  "peerDependencies": {
    "eslint": "^8.9.0",
    "prettier": "^2.0.0",
    "typescript": "^4.6.0"
  },
  "standard-version": {
    "scripts": {
      "postchangelog": "prettier --write ./CHANGELOG.md"
    }
  }
}
